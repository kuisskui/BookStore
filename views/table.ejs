<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= tableName %>
    </title>
</head>

<body>
    <table>
        <h2>
            <%= tableName %>
        </h2>
        <hr>
        <thead>
            <% columnName.forEach(function(data) { %>
                <tr>
                    <%= data %>
                </tr>
            <% }); %>
        </thead>
        <tbody>
            <% result.forEach(function(result) { %>
                <tr>
                    <% result.forEach(function(data){ %>
                        <td>
                            <%= data %>
                        </td>
                    <% }) %>
                </tr>
            <% }); %>
        </tbody>
    </table>
    <form action="<%= tableName %>/insert" method="get">
        <% tableName.forEach(function(tableName){ %>
            <label for="<%= tableName %>"><%= tableName %></label>
            <input type="text" id="<%= tableName %>">
        <% }) %>
    </form>
</body>

</html>