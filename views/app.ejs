<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>BookStore</title>
</head>
<body>
    <table>
        <h2>BookStore</h2>
        <hr>
        <thead>
          <tr>
            <th>Table name</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach(function(data) { %>
            <tr>
              <td><a href="/table?tableName=<%= data.name %>"><%= data.name %></a></td>
            </tr>
          <% }); %>
        </tbody>
      </table>
</body>
</html>